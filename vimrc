"""
" DISABLE COMPATIBLE MODE
"""
set nocompatible

"""
" DISABLE VIMINFO GENERATION
"""
set viminfo=

"""
" ENABLE 256 COLORS
"""
set t_Co=256

"""
" ENABLE AUTOIDENT
"""
autocmd FileType plugin indent on

"""
" ENABLE LINE NUMBER DISPLAY
"""
set number

"""
" ENABLE OXEDED COLORSCHEME
"""
colorscheme oxeded

"""
" ENABLE RULER
"""
set ruler

"""
" ENABLE SYNTAX HIGHLIGHT
"""
syntax on
